<h1>Books</h1>
<h4>title</h4>
<% @books.each do |book| %>
<%= book.title %>
<%= link_to "Destroy",destroy_books_path(book.id), method: :delete  %>
<% end %>

<h2>New book</h2>
<%= form_for(@book) do |f| %>
<p>Title</p>
<%= f.text_field :title %>
<p>Body</p>
<%= f.text_area :body %>
<%= f.submit "Create Book" %>
<% end %>